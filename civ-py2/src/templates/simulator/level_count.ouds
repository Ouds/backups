{% extends "base.ouds" %}{% load i18n read %}{% block title %} {% trans '级别总量模拟' %} {% endblock %}{% block body_id %}level_count{% endblock %}{% block content %}<div style="text-align:center">    <form method="post">        {% trans '元量' %}:&nbsp;{{ meta_form.meta }}<span class="red">{{ meta_form.meta.errors.as_text }}</span>        <input type="submit" value="{% trans '计算' %}" />    </form></div><table align="center" border="1" cellpadding="0" cellspacing="0">        <tr align="center" bgcolor="gray">        <td>{% trans '级别' %}</td>        <td>{% trans '总量' %}</td>    </tr>    {% for level in levels %}    <tr class="{% if level|divisibleby:9 %}slash{% else %}{% cycle 'red' 'blue' %}{% endif %}">        <td align="center">{{ level }}</td>        <td align="right">{{ level|level_arithmetic:meta }}</td>    </tr>    {% endfor %}    </table>{% endblock %}