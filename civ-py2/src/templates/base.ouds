<!DOCTYPE html>
<html>
    <head>
        
        {% load i18n base %}

        <link rel="Shortcut Icon" type="image/x-icon" href="/media/imgs/ouds.ico" />
        
        <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
        <meta http-equiv="ImageToolbar" content="no" />
        <meta http-equiv="Pragma" content="No-Cache" />
        <meta http-equiv="Window-Target" content="_top" />
        <meta name="ROBOTS" content="ALL" />
        <meta name="MSSmartTagsPreventParsing" content="true" />
        <meta name="Author" content="Ouds, 骛之, 长弓骛之" />
        <meta name="Copyright" content="This site's design and contents Copyright (c) 2010 OUDS." />
        <meta name="Keywords" content="{% block keywords %}{% endblock %}{% trans '文明' %}, {% trans '文化' %}, {% trans '战略' %}, {% trans '战术' %}, {% trans '网络游戏' %}" />
        <meta name="Description" content="{% block description %}{% endblock %}{% trans '文明【古代卷】游戏网页版' %}{% trans '。' %}" />
        
        <title>{% block title %}{% endblock %} {{ city.name }} {% trans '文明【古代卷】' %}</title>
        
        <link rel="stylesheet" type="text/css" href="/media/css/base.css" media="screen" />
        {% block css %}{% endblock %}
        
        <script type="text/javascript" src="/media/js/general.js"></script>
        {% block js %}{% endblock %}
    
    </head>
    
    <body id="{% block body_id %}base{% endblock %}" onload="start();{% block body_load %}{% endblock %}">
        
        <table id="container" align="center">

            <tr>

                <td colspan="2" id="general">
                    {% general user city.id %}
                </td> {# end general #}

            </tr>

            <tr>

                <td id="city_info">
                    {% city_info city.id %}
                </td> {# end city_info #}

                <td id="content">
                    {% block content %} {% endblock %}
                </td> {# end content #}

            </tr>

        </table> {# end container #}

    </body>
</html>
